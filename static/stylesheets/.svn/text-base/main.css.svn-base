}

.incorrect{
color:red;
}

.correct{
color:#005B8B;
}

.main {
text-align: center; 
padding-right: 80px;
padding-top: 10px;
margin-left:-50px;

}

.metaweb{

padding-bottom:5px;
border-bottom-color:#EEEEEE;
border-bottom-style:dashed;
